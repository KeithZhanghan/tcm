<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="jz.entity.TechnologyType" table="technology_type" schema="graduation">
        <id name="technologyTypeId">
            <column name="technology_type_id" sql-type="varchar(255)"/>
        </id>
        <property name="technologyTypeName">
            <column name="technology_type_name" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="technologyClassify">
            <column name="technology_classify" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="isValid">
            <column name="is_valid" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>